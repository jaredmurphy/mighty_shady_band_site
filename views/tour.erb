  <div class="col s12 about_card">
    <% @tour_dates.each do |event| %>
      <div class="row">
        <div class="col s12 m12">
          <div class="card blue-grey col12">
            <div class="card-action white-text col s12">
              <span class="card-title"><%= event["title"]%></span>
              <br />
              <div class="card-action white-text col s6">
                <img src='<%= event["artists"][0]["thumb_url"]%>' class="tour_pic"/>
              </div>
              <div class="card-action white-text col s6">
                <p class="white-text"><%=event["formatted_datetime"]%></p>
                <% event["artists"].each do |artist| %>
                  <p><%= artist["name"]%></p>
                <% end %>
                <p><a href=<%= event["facebook_rsvp_url"]%>> RSVP on Facebook </a></p>
              </div>

            </div>

          </div>
        </div>
      </div>
   <% end %>
  </div>
